<h3 class="center">Zip Codes</h3>

<div class="row">
  <div class="col s12 m4">
    <div class="card-panel">
      <h5>Highest Income Zips:</h5>
      <ol>
        <% @high_incomes.each do |zip| %>
        <li><%= link_to zip.id, zip_path(zip) %>, <%= zip.mean_income %></li>
        <% end %>
      </ol>
    </div>
  </div>
  <div class="col s12 m4">
    <div class="card-panel">
      <h5>Lowest Income Zips:</h5>
      <ol>
        <% @low_incomes.each do |zip| %>
        <li><%= link_to zip.id, zip_path(zip) %>, <%= zip.mean_income %></li>
        <% end %>
      </ol>
    </div>
  </div>
  <div class="col s12 m4">
    <div class="card-panel">
      <h5>Highest Pop Zips:</h5>
      <ol>
        <% @highest_pops.each do |zip| %>
        <li><%= link_to zip.id, zip_path(zip) %>, <%= zip.n1 %></li>
        <% end %>
      </ol>
    </div>
  </div>
</div>

